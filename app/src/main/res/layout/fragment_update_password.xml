<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".view.UpdatePasswordFragment">

    <include
        android:id="@+id/include_passwordbg"
        layout="@layout/bg_layout"/>


    <TextView
        android:id="@+id/txtv_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update Password"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/_160adp"
        android:fontFamily="@font/arialbold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="@dimen/_20asp"
        android:textColor="@color/black"
        />



</androidx.constraintlayout.widget.ConstraintLayout>